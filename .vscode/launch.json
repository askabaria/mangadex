  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/mangadex.ts",
      "preLaunchTask": "npm: build",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "args": [
        "--configuration", "./.mangadex/dev-cfg.nix"
        ,"--target", "./.mangadex/target"
        , "--run", "dev", "interactive"
        , "--dev-fetch", "33ffea1c-1dbd-4355-b318-465601c1f061"
        , "--dev-search-off", "Imperfect"
        , "--env-lang", "en"
      ]
    }
  ]
}
